local cfg = {}
local sysver = "0.2.9" -- this version is numbers of github commits lol
local loaderver = "1.01" -- ml grub
local biosver = "0.0.1" -- bios is not using

function cfg.getSysVer()
    return sysver
end

function cfg.getLoaderVer()
    return loaderver
end

function cfg.getBiosVer()
    return biosver
end

return cfg
